}
{
    <Dbi> t1=<Id> {  Nodo tmp = new Nodo(t1.image);
                     tmp.setNumNodo(graxml.contador++);
                     tmp.setValor("0");
                     nodo1 = tmp;
                  }
    <Dbf> nodo1=conper(nodo1) {  hijo2.addHijo(nodo1);

Nodo conper(Nodo hijo3 ):
{
    Token t1;
}
{
     <Obusui> t1=<Id> { Nodo tmp = new Nodo(t1.image);
                       tmp.setNumNodo(graxml.contador++);
                       tmp.setValor("0");
                       hijo3.addHijo(tmp);
                      } 
